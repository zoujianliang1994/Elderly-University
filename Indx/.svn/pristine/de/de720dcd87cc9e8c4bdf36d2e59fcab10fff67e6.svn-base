    font-size: 14px;
    font-weight: normal;
    font-stretch: normal;
    line-height: 0px;
    letter-spacing: 0px;
    color: #ffffff;
}
.back-btn{
    width: 8rem;
    height: 3rem;
    background-color: #ffffff;
    border: solid 1px #3879f7;
    font-size: 14px;
    font-weight: normal;
    font-stretch: normal;
    line-height: 0px;
    letter-spacing: 0px;
    color: #3879f7;
    cursor: pointer;

}
