.operationBox {
  color: #596980;
  height: 40px;
  width: 100%;
  font-size: 13px;
}
.operationBox .sTree .input {
  height: 39px;
  border-right: 0;
}
.operationBox .layui-btn {
  font-size: 13px;
}
.operationBox .searchPackage {
  border: 1px solid #ccc;
}
.operationBox .searchBox .search {
  /*border-right: 0;*/
  width: 21rem;
  box-sizing: border-box;
   height: 38px;;
}
.operationBox .searchBox .search-min {
    /*border-right: 0;*/
    width: 16rem;
    box-sizing: border-box;
    height: 3rem;
}
.operationBox .searchBox .srarchBtn {
  margin: 0px 1rem;
  text-align: center;
  line-height: 38px;
  cursor: pointer;
    width: 8rem;
    height: 38px;
  background-color: #3779f7;
  border: solid 0rem #3879f7;
  color: #fff;
}


.operationBox .searchBox .srarchBtn:hover {
  /*color: #000;*/
}
@media screen and (max-width: 1200px) {
  .searchBox .search {
    width: 140px!important;
  }
}
/* table */
.layui-table {
  color: #333;
}
.layui-table thead th {
  text-align: center;
    font-weight: bold;
}
.layui-table .tab-btn {
  text-align: center;
  cursor: pointer;
  font-size: 16px;
}
.layui-table .layui-icon {
  padding: 5px;
}
.layui-table .layui-icon:hover {
  color: #2795cd;
}
.laypageBox {
  overflow: hidden;
  padding-bottom: 20px;
}
.editHtml {
  width: 400px;
}
.sTreeBox {
  width: 100%;
  /*height: 300px;*/
}
/*men-list*/
.menu-list {
  overflow: auto;
  position: absolute;
  height: 100%;
  width: 100%;
}
.menu-list .tree {
  margin: 10px 0;
}
/*edit_menu*/
.edit_menu {
  padding: 2.5rem;
  color: #000;
}
.btnbox {
  margin: 20px 0;
  text-align: center;
}
/*form*/
label .require,label .red-star {
  font-size: 15px;
    color: #f63b3b;
    padding-right: 4px;
    font-style: normal;
}

.layui-form {
  font-size: 14px;
}
.layui-form .layui-input-block {
    text-align: left;
  line-height: 38px;
  color: #333;
}
.select-linkage{
    width: 100%;
    height: 38px;
    color: #666;
}
.layui-form .label-long .layui-form-label {
  width: 190px;
  text-align: left;
}
.layui-form .label-long .layui-input-block {
  margin-left: 220px;
}
.layui-btn-blue {
  background: #108ee9 !important;
  color: #fff;
}
.layui-btn-blue:hover {
  color: #fff;
  opacity: 0.8;
  border: 0;
}
.layui-tree .layui-tree-branch {
  display: none;
}
.layui-tree .layui-tree-leaf {
  opacity: 0;
}
.layui-tree .active {
  color: #108ee9;
}
.layui-form-radio{
    line-height: 30px;
}
.isShow{
    display: none;
}
.tuition_fee{
    width: 50px;
}
.btnbox .layui-btn-primary {
  color: #fff;
  border: 1px solid #fff;
}
.btnbox .layui-btn-primary:hover {
  border: 1px solid #fff;
}
.btnbox #sure,.btnbox #sonSure,.btnbox #hintSure ,.btnbox #next,.btnbox #go_login,.btnbox #addList,.btnbox .go-join {
    border-radius: 0px;
    width: 8rem;
    height: 38px;
    line-height: 38px;
    background-color: #3779f7;
    border: solid 1px #3879f7;

}
.hint-content{
    font-size: 20px;
    text-align: center;
    padding: 20px 10px;
}
#sfle03{
 width: 30rem;
    height: 5rem;
}
.stepflex{
    text-align: center;
    margin: 30px auto;
}
.stepflex dl{
    float: left;
    position: relative;
    width: 10rem;
    top: -5px;
}
.s-num{
    position: relative;
    margin: -15px auto 0;
    color: #dce3eb;
    font-weight: 700;
    font-size: 16px;
    width: 30px;
    height: 30px;
    height: 30px;
}
.s-text{
    position: absolute;
    left: 0rem;
    width: 10rem;
    color: #999999;
    font-size: 12px;
}
.password-reset-line{
    border-top: 2px solid #dce3eb; float: left;width: 4rem;
}
.doing .password-reset-line{
    border-top: 2px solid #3879f7;
}
.doing i,.doing .s-text{
    color: #3879f7;
}
.password-reset-tab .form-id{
    display: block;
}
.password-reset-tab .form-reset,.password-reset-tab .succsee-reset{
    display: none;
}
.reset-item{
    width: 23rem;
    margin:auto;
}
.success-content{
    width: 400px;
    height: 60px;
    margin: auto;
}
.success-icon{
    width: 60px;
    height: 60px;
    float: left;
}
.success-icon i{
    font-size: 60px;
    color: #97c34f;;
}
.success-text{
    width: 190px;
    height: 60px;
    float: left;
    padding:0 20px;
}
.success-text p{
    font-size: 20px;
    color: #333333;
    padding: 6px 0;
}
.success-text .time-hint{
    font-size: 14px;
    color: #333333;
    height: 20px;
}
#go_login{
    width: 5rem;
    height:2rem;
}
.btn-login{
    width: 80px;
    height: 60px;
    float: left;
}

.btnbox #cancel,.btnbox #sonCancel {
    border-radius: 0px;
    width: 8rem;
    height: 3rem;
    background-color: #ffffff;
    border: solid 1px #3879f7;
    color: #3879f7;

}
.table-check {
  width: 15px;
  height: 15px;
}
.add-btn{
    background-color: #ffc020;
    width: 8rem;
    height: 38px;
    border: solid 0rem #ffc020;
    cursor: pointer;
    font-size: 14px;
    font-weight: normal;
    font-stretch: normal;
    line-height: 0px;
    letter-spacing: 0px;
    color: #ffffff;
