package com.zhimu.dao.entity.tasks;

import java.sql.Timestamp;

import com.zhimu.dao.entity.BaseEntity;

/**
 *   业务流程数据表实体（sys_tasks）
 * @author Weiyunchao
 *
 * 2017年8月10日下午2:14:11
 */
public class Tasks extends BaseEntity{

	private String businessId;//业务ID
	private String title;//标题
	private String content;//内容
	private String approveId;//审批人ID
	private String approveName;//审批人名称
	private Timestamp approveTime;//审批时间
	private String userId;//指定人员审批
	private String type;//业务类型
	private String status;//状态
	private String isDel;//是否删除
	private String url;//跳转审批页面链接
	public String getBusinessId() {
		return businessId;
	}
	public void setBusinessId(String businessId) {
		this.businessId = businessId;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getApproveId() {
		return approveId;
	}
	public void setApproveId(String approveId) {
		this.approveId = approveId;
	}
	public String getApproveName() {
		return approveName;
	}
	public void setApproveName(String approveName) {
		this.approveName = approveName;
	}
	public Timestamp getApproveTime() {
		return approveTime;
	}
	public void setApproveTime(Timestamp approveTime) {
		this.approveTime = approveTime;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public String getIsDel() {
		return isDel;
	}
	public void setIsDel(String isDel) {
		this.isDel = isDel;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getUrl() {
		return url;
	}
	public void setUrl(String url) {
		this.url = url;
	}
	
}