/******** 鏉垮潡 *******/
.section {
    padding: 10px;
    background-color: #fff;
    overflow: hidden;
    clear:both;
}

.section.lborder {
    border-left: 1px solid #efefef;
}

.section .tit {
    display: block;
    margin: 5px 0 0 5px;
    padding-left: 10px;
    width: 100%;
    height: 20px;
    line-height: 20px;
    font-size: 16px;
    font-weight: 400;
    border-left: 3px solid #0281c4;
    text-align: left;
}

.section .list {
	overflow:hidden;
}

.section .list .item {
    margin: 10px 0;
    padding-left: 15px;
    overflow: hidden;
}

.paneBox .paneBox-item {
    width: 360px;
    float: left;
    padding: 0 15px;
    height: 250px;
     overflow: hidden;
}

.section .list .item a {
    float: left;
    display: block;
    width: 330px;
    color: #666;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-break: break-all;
    text-align:left
}

.section .list .item a:hover {
    color: #52a8d7;
}

.section .list .item span {
    float: right;
    display: block;
    padding-left: 10px;
    width: 80px;
    font-size: 12px;
    color: #999;
}

.section-notice {
    padding: 15px;
    height: 320px;
    overflow: hidden;
    background:#fff;
}

.section-notice .right {
    float: right;
    width: 620px;
}

.swiper-slide .item {
    display: block;
    width: 275px;
    height: 290px;
    float:left;
    background-size: cover;
}

.section-notice .tabs {
    position: relative;
    margin-top: -15px;
    height: 50px;
    border-bottom: 1px solid #2f7493;
}

.section-notice .tabs a {
    float: left;
    display: inline-block;
    width: 49%;
    height: 50px;
    line-height: 50px;
    font-size: 17px;
    text-align: left;
    color: #666;
    text-indent: 25px;
}

.section-notice .tabs .active {
    color: #0083c6;
    border-bottom: 4px solid #0083c6;
}

.section-notice .pane {
    display: none;
}

.section-notice .pane.active {
    display: block;
}

.section-board {
    padding: 0px 0 20px 0;
}

.section-board .tit {
    margin: 0;
    padding: 0;
    height: 50px;
    text-align: left;
    font-weight: 300;
    line-height: 50px;
    font-size: 17px;
    color: #666;
    border-left: none;
    border-bottom: 1px solid #2f7493;
    text-indent: 20px;
}

.btn-board {
    display: block;
    width: 100%;
    height: 100px;
    background-size: 100% 100%;
    -webkit-transition: background-color ease-out .15s;
    -moz-transition: background-color ease-out .15s;
    -ms-transition: background-color ease-out .15s;
    -o-transition: background-color ease-out .15s;
    transition: background-color ease-out .15s;
    background-repeat: no-repeat;
    position: relative;
    margin-top: 20px;
}
