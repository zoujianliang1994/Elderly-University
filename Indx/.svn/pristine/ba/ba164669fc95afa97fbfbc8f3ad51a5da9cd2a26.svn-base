/******** 鍒嗛〉 *******/
.pagination {
    font-size: 0;
    -moz-user-select: none;
    -khtml-user-select: none;
    user-select: none;
}

.pagination .next {
    background: url("../images/icon32.png") no-repeat 0px -588px;
}

.pagination .prev {
    background: url("../images/icon32.png") no-repeat 0px -546px;
}

.pagination a {
    float: left;
}

.pagination ul {
    float: left
}

.pagination li, .pagination a, .pagination i {
    cursor: pointer;
    float: left;
    color: #52a8d7;
    margin: 0 3px;
    width: 35px;
    height: 35px;
    line-height: 32px;
    font-size: 16px;
    text-align: center;
    border: 1px solid #eee;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    vertical-align: middle;
}

.pagination li:hover {
    border-color: #278dcd;
}

.pagination .active {
    border-color: #278dcd;
    background-color: #278dcd;
    color: #fff;
}

.pagination .ellipsis {
    background: url("../images/icon20.png") no-repeat 7px -353px;
}

.pagination .ellipsis:hover {
    background: url("../images/icon20.png") no-repeat -23px -353px;
}

