}
.shadowText {
    position: absolute;
    right: 0;
    bottom: 0;
    width:100%;
    background: rgba(1, 1, 1, 0.5);
    filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr=#7f111111,endcolorstr=#7f111111);
