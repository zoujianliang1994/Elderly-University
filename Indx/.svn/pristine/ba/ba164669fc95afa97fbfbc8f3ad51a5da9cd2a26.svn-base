.section-icon {
    overflow: hidden;
}

.section-icon .icon-box {
    margin: 10px;
    border: none;
    background: transparent;
    -webkit-transition: background-color ease-out .15s;
    -moz-transition: background-color ease-out .15s;
    -ms-transition: background-color ease-out .15s;
    -o-transition: background-color ease-out .15s;
    transition: background-color ease-out .15s;
}

.section-icon .icon-box:hover {
    background-color: rgba(200, 200, 200, .15);
}

.section-icon .icon-box .icon {
    display: block;
    margin: 0 auto;
    width: 84px;
    height: 84px;
