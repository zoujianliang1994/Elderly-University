package com.zhimu.commons.constant;

/**
 * 公告通知发布范围枚举常量类
 */
public enum NoticeConstant {
    /**
     * 所有人
     */
    ALL_PEOPLE(1,"所有人"),
    /**
     * 所有老师
     */
    ALL_TEACHER(2,"所有老师"),
    /**
     * 所有学员
     */
    ALL_STUDENT(3,"所有学员"),
    /**
     * 校区
     */
    CAMPUS(4,"校区"),
    /**
     * 班级
     */
    CLASS_AND_GRADE(5,"班级"),
    /**
     * 人员
     */
    STUDENT_AND_TEACHER(6,"人员");

    private NoticeConstant(Integer value,String name){
        this.value = value;
        this.name = name;
    }
    private final Integer value;
    private final String name;

    public Integer getValue() {
        return value;
    }

    public String getName() {
        return name;
    }
}
